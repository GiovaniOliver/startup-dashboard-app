{


  "homepage": "https://github.com/GiovaniOliver/NCE-Dashboard-Manager.git",


  "name": "startup-dashboard-app",


  "version": "0.1.0",


  "private": true,


  "dependencies": {


    "@emotion/react": "^11.9.0",


    "@emotion/styled": "^11.8.1",


    "@mui/icons-material": "^7.3.5",


    "@mui/material": "^7.3.5",


    "@mui/x-data-grid": "^8.18.0",


    "@testing-library/jest-dom": "^5.16.4",


    "@testing-library/react": "^13.1.1",


    "@testing-library/user-event": "^13.5.0",


    "near-api-js": "^0.44.2",


    "react": "^18.1.0",


    "react-beautiful-dnd": "^13.1.1",


    "react-dom": "^18.1.0",


    "react-router-dom": "^6.3.0",


    "react-scripts": "5.0.1",


    "recharts": "^3.4.1",


    "sass": "^1.51.0",


    "uuid": "^13.0.0",


    "web-vitals": "^2.1.4"


  },


  "scripts": {


    "start": "react-app-rewired start",


    "build": "react-app-rewired build",


    "test": "react-app-rewired test",


    "test:coverage": "react-app-rewired test --coverage --watchAll=false",


    "test:watch": "react-app-rewired test --watch",


    "eject": "react-scripts eject",


    "lint": "eslint src/**/*.{js,jsx}",


    "lint:fix": "eslint src/**/*.{js,jsx} --fix",


    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss,md}\"",


    "analyze": "source-map-explorer 'build/static/js/*.js'",


    "build:prod": "GENERATE_SOURCEMAP=false npm run build",


    "build:analyze": "npm run build && npm run analyze",


    "serve": "serve -s build -l 3000",


    "docker:build": "docker build -t startup-dashboard-app .",


    "docker:run": "docker run -p 80:80 startup-dashboard-app",


    "docker:dev": "docker-compose up app-dev",


    "docker:prod": "docker-compose up app",


    "deploy:vercel": "vercel --prod",


    "deploy:netlify": "netlify deploy --prod",


    "precommit": "npm run lint && npm test -- --watchAll=false"


  },


  "eslintConfig": {


    "extends": [


      "react-app",


      "react-app/jest"


    ]


  },


  "browserslist": {


    "production": [


      ">0.2%",


      "not dead",


      "not op_mini all"


    ],


    "development": [


      "last 1 chrome version",


      "last 1 firefox version",


      "last 1 safari version"


    ]


  },


  "jest": {


    "collectCoverageFrom": [


      "src/**/*.{js,jsx}",


      "!src/index.js",


      "!src/reportWebVitals.js",


      "!src/**/*.test.{js,jsx}",


      "!src/**/__tests__/**"


    ],


    "coverageThreshold": {


      "global": {


        "branches": 80,


        "functions": 80,


        "lines": 80,


        "statements": 80


      }


    },


    "coverageReporters": [


      "text",


      "lcov",


      "html"


    ]


  },


  "devDependencies": {


    "buffer": "^6.0.3",


    "crypto-browserify": "^3.12.1",


    "process": "^0.11.10",


    "react-app-rewired": "^2.2.1",


    "stream-browserify": "^3.0.0"


  }


}


